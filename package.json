{
  "name": "rails-go-to-factory-bot",
  "displayName": "Rails go to factory bot",
  "description": "A VS Code extension to navigate to Factory Bot definitions in Rails projects.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.4"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/frenesim/rails-go-to-factory-bot"
  },
  "publisher": "Formigagalatica",
  "main": "./extension.js",
  "activationEvents": [
    "onCommand:rails-go-to-factory-bot.goTo"
  ],
  "contributes": {
    "commands": [
      {
        "command": "rails-go-to-factory-bot.goTo",
        "title": "Rails Go to Factory"
      }
    ],
    "keybindings": [
      {
        "command": "rails-go-to-factory-bot.goTo",
        "key": "ctrl+k ctrl+f",
        "mac": "cmd+g cmd+f",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "pluralize": "^8.0.0"
  }
}
